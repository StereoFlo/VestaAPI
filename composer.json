{
  "name": "stereoflo/VestaManage",
  "description": "VestaCP client for Laravel",
  "keywords": [
    "framework",
    "laravel",
    "hosting",
    "vesta",
    "panel",
    "api"
  ],
  "type": "library",
  "require": {
    "laravel/framework": "^5.3"
  },
  "license": "GPL-3.0",
  "support": {
    "issues": "https://github.com/stereoflo/VestaManage/issues",
    "source": "https://github.com/stereoflo/VestaManage"
  },
  "authors": [
    {
      "name": "Stereo Flo",
      "email": "stereoflo@stereoflo.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "VestaManage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VestaManage\\Providers\\VestaServiceProvider"
      ],
      "aliases": {
        "Dashboard": "VestaManage\\Facades\\Vesta"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
